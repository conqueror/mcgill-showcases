{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Experiment Log Row",
  "type": "object",
  "required": [
    "run_name",
    "timestamp_utc",
    "split_strategy",
    "primary_metric",
    "primary_metric_value"
  ],
  "properties": {
    "run_name": {"type": "string", "minLength": 1},
    "timestamp_utc": {"type": "string", "minLength": 1},
    "split_strategy": {"type": "string", "minLength": 1},
    "primary_metric": {"type": "string", "minLength": 1},
    "primary_metric_value": {"type": "number"},
    "notes": {"type": "string"}
  },
  "additionalProperties": true
}
