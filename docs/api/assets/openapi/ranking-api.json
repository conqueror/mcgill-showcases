            "title": "Features",
            "type": "object"
          },
          "player_id": {
            "minLength": 1,
            "title": "Player Id",
            "type": "string"
          }
        },
        "required": [
          "player_id"
        ],
        "title": "PlayerRecord",
        "type": "object"
      },
      "PlayerScore": {
        "properties": {
          "player_id": {
            "title": "Player Id",
            "type": "string"
          },
          "score": {
            "title": "Score",
            "type": "number"
          }
        },
        "required": [
          "player_id",
          "score"
        ],
        "title": "PlayerScore",
        "type": "object"
      },
      "RankResponse": {
        "properties": {
          "rankings": {
            "items": {
              "$ref": "#/components/schemas/PlayerRank"
            },
            "title": "Rankings",
            "type": "array"
          }
        },
        "required": [
          "rankings"
        ],
        "title": "RankResponse",
        "type": "object"
      },
      "ScoreRequest": {
        "properties": {
          "records": {
            "items": {
              "$ref": "#/components/schemas/PlayerRecord"
            },
            "minItems": 1,
            "title": "Records",
            "type": "array"
          }
        },
        "required": [
          "records"
        ],
        "title": "ScoreRequest",
        "type": "object"
