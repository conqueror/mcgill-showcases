        "required": [
          "feature_names"
        ],
        "title": "ModelSchemaResponse",
        "type": "object"
      },
      "PlayerRank": {
        "properties": {
          "player_id": {
            "title": "Player Id",
            "type": "string"
          },
          "rank": {
